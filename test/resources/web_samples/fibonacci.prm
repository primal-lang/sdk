fibonacci(n) = if (n == 0)
                   []
               else
                   fibonacci(n - 1) + [fibonacciHelper(n - 1)]

fibonacciHelper(n) = if (n == 0)
                         1
                     else if (n == 1)
                         1
                     else
                         fibonacciHelper(n - 1) + fibonacciHelper(n - 2)

main = fibonacci(10)